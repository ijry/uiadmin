{
    "name": "uniadmin/uniadmin-core",
    "description": "渐进式开源低代码零前端后台",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "jry",
            "email": "ijry@qq.com"
        }
    ],
    "require": {
        "topthink/framework": "^6.0.0",
        "topthink/think-migration": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "uniadmin\\core\\": "src/"
        },
        "files":[
            "src/core/src/function.php"
        ] 
    },
    "extra": {
        "think": {
            "services": [
                "uniadmin\\core\\MyService"
            ],
            "config":{
                "uniadmin": "src/config.php",
                "menu": "src/menu.json"
            }
        }
    }
}
