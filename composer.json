{
    "name": "uiadmin/uiadmin",
    "description": "渐进式开源低代码零前端后台",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "jry",
            "email": "ijry@qq.com"
        }
    ],
    "require": {
        "topthink/framework": "^6.0.0",
        "casbin/think-authz": "^1.2.2"
    },
    "autoload": {
        "psr-4": {
            "uiadmin\\core\\": "src/core/src/",
            "uiadmin\\auth\\": "src/auth/src/",
            "uiadmin\\cms\\": "src/cms/src/"
        },
        "files":[
            "src/core/src/function.php"
        ] 
    },
    "extra": {
        "think": {
            "services": [
                "uiadmin\\core\\MyService",
                "tauthz\\TauthzService"
            ],
            "config":{
                "uiadmin": "src/core/src/config.php"
            }
        }
    }
}
