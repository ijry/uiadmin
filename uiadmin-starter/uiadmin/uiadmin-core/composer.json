{
    "name": "uiadmin/uiadmin-core",
    "description": "渐进式开源低代码零前端后台核心模块",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "jry",
            "email": "ijry@qq.com"
        }
    ],
    "require": {
        "topthink/framework": "^6.0.0",
        "topthink/think-migration": "^3.0",
        "topthink/think-view": "^1.0.14",
        "firebase/php-jwt": "^5.5.1",
        "guzzlehttp/guzzle": "^6.3",
        "jaguarjack/migration-generator": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "uiadmin\\core\\": "src/"
        },
        "files":[
            "src/function.php"
        ] 
    },
    "extra": {
        "think": {
            "services": [
                "uiadmin\\core\\Service"
            ],
            "config":{
                "uiadmin": "src/config.php"
            }
        }
    }
}
