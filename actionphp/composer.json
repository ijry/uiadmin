{
    "name": "actionphp",
    "description": "the new thinkphp framework",
    "type": "project",
    "keywords": [
        "framework",
        "initadmin",
        "actionphp"
    ],
    "homepage": "https://initadmin.net",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "jry",
            "email": "ijry@qq.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "topthink/framework": "5.1.*",
        "topthink/think-queue": "^2.0",
        "firebase/php-jwt": "^5.0",
        "topthink/think-image": "^1.0",
        "topthink/think-migration": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "app\\": "application"
        }
    },
    "extra": {
        "think-path": "thinkphp"
    },
    "config": {
        "preferred-install": "dist"
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
