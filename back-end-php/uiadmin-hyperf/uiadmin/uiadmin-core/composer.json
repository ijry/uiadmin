{
    "name": "uiadmin/uiadmin-core",
    "description": "渐进式开源低代码零前端后台核心模块",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "jry",
            "email": "ijry@qq.com"
        }
    ],
    "require": {
        "firebase/php-jwt": "^6.9.0"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "uiadmin\\core\\": "src/"
        },
        "files":[
            "src/function.php"
        ] 
    },
    "extra": {
        "hyperf": {
            "config": "uiadmin\\core\\ConfigProvider"
        },
        "laravel": {
            "providers": [
                "uiadmin\\core\\LrvServiceProvider"
            ],
            "config":{
                "uiadmin": "src/config.php"
            }
        }
    }
}
